
namespace Am.Ui 
{

    class MyBase() {
        import Am.Lang

        override fun toString(): String {
            return "base"
        }
    }

    class MyChild() : MyBase {
        import Am.Lang

        override fun toString(): String {
            return "child"
        }

        fun toStringBase(): String {
            return base.toString()
        }
    }

    native class Startup {
        import Am.Lang
        import Am.Threading

        static fun main() {

            var child = new MyChild()
            child.toString().println()
            child.toStringBase().println()

            "Ui started".println()
            var w = Window.openWindow(150US, 150US, null)

            var b = new Button()
            "button created".println()
            w.setRootView(b)
            "root view set".println()

            Thread.sleep(2000L)            
        }        

    }
}

